{
	"name": "console-wrap-php-js-python",
	"displayName": "Console Wrap PHP/JS/Python",
	"description": "This extension helps you easily create log statements ( print_r(), console.log(), print() )",
	"version": "0.0.6",
	"publisher": "Code-Elements",
	"author": {
		"name": "Code-Elements"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Code-Elements/Console-Wrap-PHP-JS-Python.git"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Programming Languages",
		"Debuggers",
		"Other"
	],
	"keywords": [
		"JavaScript",
		"console.log",
		"Python",
		"Print",
		"Log",
		"PHP",
		"echo <pre>"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onCommand:console-wrap-php-js-python.log"
	],
	"main": "./extension.js",
	"contributes": {
		"languages": [
			{
				"id": "javascript",
				"aliases": [
					"JavaScript",
					"javascript",
					"js"
				],
				"extensions": [
					".js"
				]
			},
			{
				"id": "typescript",
				"aliases": [
					"TypeScript",
					"typescript",
					"ts"
				],
				"extensions": [
					".ts"
				]
			},
			{
				"id": "php",
				"aliases": [
					"PHP",
					"php",
					"php"
				],
				"extensions": [
					".php"
				]
			},
			{
				"id": "python",
				"aliases": [
					"Python",
					"python",
					"py"
				],
				"extensions": [
					".py"
				]
			}
		],
		"keybindings": [
			{
				"command": "console-wrap-php-js-python.log",
				"key"    : "ctrl+shift+q",
				"mac"    : "cmd+shift+q",
				"when"   : "editorTextFocus"
			}
		]
	},
	"scripts": {
		"pretest": "npm run lint",
		"test"   : "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode"        : "^1.66.0",
		"@types/glob"          : "^7.2.0",
		"@types/mocha"         : "^9.1.0",
		"@types/node"          : "14.x",
		"eslint"               : "^8.11.0",
		"glob"                 : "^7.2.0",
		"mocha"                : "^9.2.2",
		"typescript"           : "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}